{
    "name": "phpinnacle/buffer",
    "type": "library",
    "description": "PHPinnacle binary buffer implementation",
    "keywords": [
        "phpinnacle", "binary", "buffer", "binary data", "pack", "unpack"
    ],
    "homepage": "https://github.com/phpinnacle/buffer",
    "license": "MIT",
    "authors": [
        {
            "name": "PHPinnacle",
            "email": "dev@phpinnacle.com",
            "homepage": "https://phpinnacle.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "PHPinnacle\\Buffer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPinnacle\\Buffer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
